[
  {
    "key": "cmd+alt+shift+left",
    "command": "moveActiveEditor",
    "args": {
      "to": "left",
      "by": "tab",
      "value": 1
    }
  },
  {
    "key": "cmd+alt+shift+right",
    "command": "moveActiveEditor",
    "args": {
      "to": "right",
      "by": "tab",
      "value": 1
    }
  },
  {
    "key": "alt+m down",
    "command": "merge-conflict.next",
    "when": "editorTextFocus"
  },
  {
    "key": "alt+m up",
    "command": "merge-conflict.previous",
    "when": "editorTextFocus"
  },
  {
    "key": "alt+m enter",
    "command": "merge-conflict.accept.selection",
    "when": "editorTextFocus"
  },
  {
    "key": "alt+m 1",
    "command": "merge-conflict.accept.current",
    "when": "editorTextFocus"
  },
  {
    "key": "alt+m 2",
    "command": "merge-conflict.accept.incoming",
    "when": "editorTextFocus"
  },
  {
    "key": "alt+m 3",
    "command": "merge-conflict.accept.both",
    "when": "editorTextFocus"
  },
  {
    "key": "pageup",
    "command": "cursorMove",
    "args": {
      "to": "up",
      "by": "line",
      "value": 10
    },
    "when": "editorTextFocus"
  },
  {
    "key": "pagedown",
    "command": "cursorMove",
    "args": {
      "to": "down",
      "by": "line",
      "value": 10
    },
    "when": "editorTextFocus"
  },
  {
    "key": "shift+alt+-",
    "command": "workbench.action.decreaseViewWidth"
  },
  {
    "key": "shift+alt+=",
    "command": "workbench.action.increaseViewWidth"
  },
  {
    "key": "ctrl+cmd+s",
    "command": "cSpell.suggestSpellingCorrections"
  },
  {
    "key": "ctrl+cmd+backspace",
    "command": "workbench.action.terminal.kill",
    "when": "terminalFocus && terminalHasBeenCreated"
  },
  {
    "key": "ctrl+cmd+\\",
    "command": "workbench.action.toggleMaximizedPanel"
  },
  {
    "key": "alt+cmd+\\",
    "command": "workbench.action.movePanelToSecondarySideBar"
  },
  {
    "key": "alt+cmd+]",
    "command": "workbench.action.moveSecondarySideBarToPanel"
  },
  {
    "key": "alt+f4",
    "command": "git.unstage"
  },
  {
    "key": "alt+f6",
    "command": "git.stage"
  },
  {
    "key": "ctrl+n",
    "command": "explorer.newFile",
    "when": "!editorFocus"
  },
  {
    "key": "ctrl+shift+n",
    "command": "explorer.newFolder",
    "when": "!editorFocus"
  },
  {
    "key": "ctrl+shift+b",
    "command": "editor.action.insertSnippet",
    "args": {
      "snippet": "// #region $0\n$TM_SELECTED_TEXT\n// #endregion"
    },
    "when": "editorHasSelection && editorTextFocus && !editorReadonly",
  }
]