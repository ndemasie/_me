// Place your key bindings in this file to override the defaults
[
  {
    "key": "ctrl+n",
    "command": "explorer.newFile",
    "when": "!editorFocus"
  },
  {
    "key": "ctrl+shift+n",
    "command": "explorer.newFolder",
    "when": "!editorFocus"
  },
  {
    "key": "shift+alt+=",
    "command": "workbench.action.increaseViewSize"
  },
  {
    "key": "shift+alt+-",
    "command": "workbench.action.decreaseViewSize"
  },
  {
    "key": "ctrl+cmd+s",
    "command": "cSpell.suggestSpellingCorrections"
  },
  {
    "key": "ctrl+cmd+backspace",
    "command": "workbench.action.terminal.kill",
    "when": "terminalFocus && terminalHasBeenCreated"
  },
  {
    "key": "ctrl+cmd+\\",
    "command": "workbench.action.toggleMaximizedPanel"
  },
  {
    "key": "alt+f4",
    "command": "git.unstage"
  },
  {
    "key": "alt+f6",
    "command": "git.stage"
  },
  {
    "key": "f10",
    "command": "cursorMove",
    "args": {
      "to": "down",
      "by": "wrappedLine",
      "value": 8,
      "select": false
    },
    "when": "editorTextFocus && !editorReadonly"
  },
  {
    "key": "f11",
    "command": "cursorMove",
    "args": {
      "to": "up",
      "by": "wrappedLine",
      "value": 8,
      "select": false
    },
    "when": "editorTextFocus && !editorReadonly"
  },
  {
    "key": "ctrl+shift+b",
    "command": "editor.action.insertSnippet",
    "when": "editorHasSelection && editorTextFocus && !editorReadonly",
    "args": {
      "snippet": "// #region $0\n$TM_SELECTED_TEXT\n// #endregion"
    }
  },
  // {
  //   "key": "alt+cmd+\\",
  //   "command": "workbench.action.movePanelToSecondarySideBar"
  // },
  // {
  //   "key": "alt+cmd+]",
  //   "command": "workbench.action.moveSecondarySideBarToPanel",
  //   "when": "!editorTextFocus && !foldingEnabled"
  // },
  {
    "key": "cmd+k e",
    "command": "workbench.files.action.focusOpenEditorsView",
    "when": "workbench.explorer.outline.open"
  },
  {
    "key": "shift+cmd+e",
    "command": "workbench.files.action.focusFilesExplorer"
  },
  {
    "key": "shift+cmd+u",
    "command": "cursorRedo"
  }
]
